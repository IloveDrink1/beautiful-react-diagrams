{
  "name": "beautiful-react-diagrams",
  "version": "1.0.0",
  "description": "A collection of lightweight React components to build diagrams with ease",
  "main": "index.js",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "npm run check-types && NODE_ENV=production rollup -c rollup.config.js && node scripts/clean-dist-declarations.js",
    "build:watch": "rollup --watch --silent -c rollup.config.js",
    "start": "npm run storybook",
    "test": "mocha",
    "test:coverage": "nyc npm run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@svgr/core": "^6.3.1",
    "@svgr/plugin-jsx": "^6.3.1",
    "@svgr/plugin-svgo": "^6.3.1",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sinon": "^10.0.13",
